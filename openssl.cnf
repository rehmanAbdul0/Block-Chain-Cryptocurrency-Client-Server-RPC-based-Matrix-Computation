[ req ]
distinguished_name = req_distinguished_name
req_extensions     = v3_req
prompt             = no

[ req_distinguished_name ]
C  = US
ST = YourState
L  = YourCity
O  = YourOrganization
CN = 192.168.18.27

[ v3_req ]
subjectAltName = @alt_names

[ alt_names ]
IP.1 = 192.168.18.27
# You can add more IPs or DNS names as needed:
IP.2 = 127.0.0.1
# DNS.1 = localhost
